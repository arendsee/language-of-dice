# this is a nonsensical, but syntactically correct, script designed to test
# the simple parsing of each element of the language

x_con = 10
x_con++
x_con--
x_con += 1
x_con -= 1
x_con /= 1
x_con %= 1
y_ran := 1d6
z = false

while(true)
    1d20
        (. > 10 and not x_con > 1)  { 10 + 2 }
        (. < 10)  { 10 - 2 }
        (. >= 10) { 10 * 2 }
        (. <= 10) { 10 / 2 }
        (. == 10) { 10 % 2 }
        (. != 10) { continue }
    break
done
yield x_con
